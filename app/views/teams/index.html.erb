<h1>Index Page</h1>

<table class="table">
<thead class="table-dark">
<tr>
<th>Id</th>
<th>Name</th>
<th>Country</th>
<th>Founded</th>
<th>Description</th>
<th colspan="3">Actions</th>
</tr>
</thead>
<tbody>
 <% @teams.each do |team| %>
    <tr>
    <td><%= team.id %></td>
    <td><%= team.name %></td>
    <td><%= team.country %></td>
    <td><%= team.founded %></td>
    <td><%= team.description %></td>
    <td><%= link_to "view",team_path(team) %></td>
    <td><%= link_to "edit",new_team_path(team) %></td>
    <td><%= link_to 'Destroy', team_path(team),data: { turbo_method: :delete } %></td>
    </tr>
    <% end %>
    </tbody>
</table>